@using Proyecto_Tickets.Data
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_LayoutGeneral";
}
@model IEnumerable<Proyecto_Tickets.Models.Usuario>

<link rel="stylesheet" href="~/css/Administrador/ListaUsuarios.css" asp-append-version="true" />

<div class="content-container">
    <div class="principal">
        <div class="label1">
            <h2>Usuarios registrados</h2>
        </div>
        <div class="formulario">
            <table>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Correo</th>
                    <th>Contraseña</th>
                </tr>
                @foreach (var usuario in Model)
                {
                    <tr>
                        <td>@usuario.Id</td>
                        <td>@usuario.nombre</td>
                        <td>@usuario.Correo</td>
                        <td>@usuario.contraseña</td>
                    </tr>
                }
            </table>
        </div>
        <div class="center-button">
            <button onclick="volver()">Volver</button>
        </div>
    </div>
</div>

<script>
    function volver() {
        window.location.href = "/Administrador/Index";
    }
</script>
